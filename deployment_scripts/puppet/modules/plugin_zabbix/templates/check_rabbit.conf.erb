[rabbitmq]
log_level=CRITICAL
user=<%= scope.lookupvar('plugin_zabbix::params::openstack::rabbit_user') %>
password=<%= scope.lookupvar('plugin_zabbix::params::openstack::rabbit_password') %>
host=http://<%= scope.lookupvar('plugin_zabbix::params::host_ip') %>:15672
#OpenStack queues, Y - number of service types, N - count of *this* service, max_queues=Y*(2*N+1)
max_queues=128
